   "source": [
    "zeinuki_kakaku = input(\"税抜価格を入力してください: \")\n",
    "zeikomi_kakaku = round(int(zeinuki_kakaku) * 1.1, 0)\n",
    "if zeikomi_kakaku >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料として350円かかります\")\n",
    "    zeikomi_kakaku += 350\n",
    "\n",
    "print(f\"送料込みの価格は {zeikomi_kakaku:.0f} 円です。\")"
   ]
  }
